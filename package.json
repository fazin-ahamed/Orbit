{
  "name": "businessos",
  "version": "0.1.0",
  "description": "BusinessOS MVP - Multi-tenant SaaS Platform",
  "main": "dist/index.js",
  "scripts": {
    "build": "cd client && npm run build",
    "build:backend": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "migrate": "ts-node --transpile-only ./node_modules/.bin/knex --knexfile knexfile.ts migrate:latest",
    "migrate:make": "ts-node ./node_modules/.bin/knex migrate:make",
    "test": "vitest",
    "lint": "eslint src/**/*.ts"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.75.0",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.10.6",
    "@types/passport": "^1.0.16",
    "@types/passport-jwt": "^3.0.9",
    "@types/pg": "^8.10.9",
    "@types/speakeasy": "^2.0.10",
    "@types/stripe": "^8.0.416",
    "axios": "^1.12.2",
    "bcrypt": "^5.1.1",
    "bullmq": "^5.2.3",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "openai": "^6.3.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-saml": "^2.1.0",
    "pg": "^8.11.5",
    "redis": "^4.6.13",
    "speakeasy": "^2.0.0",
    "stripe": "^19.1.0",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "supabase": "^2.51.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3",
    "vitest": "^1.1.3"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
